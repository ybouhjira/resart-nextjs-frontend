{
  "name": "resart-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "prettier": "prettier --write ./app/**/*.{tsx,ts} ./cdk/**/*.ts",
    "prettier:watch": "prettier --watch ./app/**/*.{tsx,ts} ./cdk/**/*.ts",
    "docker-compose": "docker compose up -d",
    "cypress": "cypress open",
    "cypress:ci": "CYPRESS_BASE_URL=http://localhost:80/ cypress run",
    "create-database": "prisma migrate dev && prisma db seed"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@prisma/client": "^4.11.0",
    "@svgr/webpack": "^6.5.1",
    "@types/cypress": "^1.1.3",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.31.3",
    "swiper": "^9.0.5",
    "tailwindcss": "^3.2.6",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "cypress": "^12.7.0"
  }
}
